; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="Chest Clicker RPG"
run/main_scene="res://gui/Chest screen/Scenes/MainScene.tscn"
config/icon="res://icon.png"

[autoload]

ItemsData="*res://assets/data/ItemsData.gd"
ItemsStorage="*res://src/ItemsStorage.gd"
ItemGenerator="*res://src/ItemGenerator.gd"
StatsData="*res://assets/data/StatsData.gd"
Audio="*res://src/AudioStreamPlayer.gd"
Character="*res://src/Character.gd"
ResourceManager="*res://src/ResourceManager.gd"
EnemyData="*res://assets/data/EnemyData.gd"
Progress="*res://src/Progress.gd"
SaveManager="*res://src/SaveManager.gd"
Settings="*res://src/Settings.gd"
AchievementsData="*res://assets/data/AchievementsData.gd"
Achievements="*res://src/Achievements.gd"
Fighting="*res://src/Fighting.gd"

[display]

window/size/width=720
window/size/height=1280
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=false

[locale]

translations=PoolStringArray( "res://assets/localization/local.en.translation", "res://assets/localization/local.ru.translation" )
locale_filter=[ 0, [  ] ]

[rendering]

environment/default_environment="res://default_env.tres"
